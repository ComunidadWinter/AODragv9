''
'frmCustomKeys - Allows the user to customize keys.
'Implements class clsCustomKeys
'
'@author Rapsodius
'@date 20070805
'@version 1.0.0
'@see clsCustomKeys

Option Explicit

Private Sub Command1_Click()
Call CustomKeys.LoadDefaults
Dim i As Long

For i = 1 To CustomKeys.Count
    Text1(i).Text = CustomKeys.ReadableName(CustomKeys.BindedKey(i))
Next i
End Sub

Private Sub Command2_Click()

Dim i As Long

On Error GoTo Fallo

For i = 1 To CustomKeys.Count
    If LenB(Text1(i).Text) = 0 Then
       Call MsgBox("Hay una o mas teclas no validas, por favor verifique.", vbCritical Or vbOKOnly Or vbApplicationModal Or vbDefaultButton1, "AoDraG Online")
       Exit Sub
    End If
Next i

'TeclasModificadas = True
Call CustomKeys.SaveCustomKeys

Fallo:
Unload Me
End Sub

Private Sub Command3_Click()

Unload Me

End Sub

Private Sub Form_Load()
    
    'Call CustomKeys.LoadDefaults
    Dim i As Long
    
    For i = 1 To CustomKeys.Count
        Text1(i).Text = CustomKeys.ReadableName(CustomKeys.BindedKey(i))
    Next i
End Sub

