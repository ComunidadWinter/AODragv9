Private Sub Image4_Click()
    If Option1.Value = True Then
        Call SendData("PT41")
        Party.reparto = 1
    ElseIf Option2.Value = True Then
        Call SendData("PT42")
        Party.reparto = 2
    ElseIf Option3.Value = True Then
        Call SendData("PT43")
        Party.reparto = 3
    End If
    frmParty.Visible = False
    Unload frmParty
End Sub

